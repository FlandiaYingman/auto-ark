<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.2" fx:controller="top.anagke.auto_android.img.CannyTester">
    <bottom>
        <GridPane hgap="10.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES"/>
                <ColumnConstraints hgrow="ALWAYS"/>
                <ColumnConstraints hgrow="SOMETIMES"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label fx:id="label2" text="0.0" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
                <Label fx:id="label1" text="0.0" GridPane.columnIndex="2"/>
                <Slider fx:id="slider1" max="255.0" GridPane.columnIndex="1">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Slider>
                <Slider fx:id="slider2" max="255.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="Threshold 1"/>
                <Label text="Threshold 2" GridPane.rowIndex="1"/>
                <Label fx:id="label3" text="0.0" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
                <Label text="Blur Size" GridPane.rowIndex="2"/>
                <Slider fx:id="slider3" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            </children>
            <BorderPane.margin>
                <Insets/>
            </BorderPane.margin>
        </GridPane>
    </bottom>
    <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <top>
        <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <right>
                <ImageView fx:id="imageView2" fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true"
                           preserveRatio="true" BorderPane.alignment="CENTER"/>
            </right>
            <left>
                <ImageView fx:id="imageView1" fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true"
                           preserveRatio="true" BorderPane.alignment="CENTER"/>
            </left>
        </BorderPane>
    </top>
</BorderPane>
